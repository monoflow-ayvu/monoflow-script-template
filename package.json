{
  "name": "monoflow-script-env",
  "version": "1.0.0",
  "license": "EULA",
  "private": true,
  "scripts": {
    "build": "webpack --mode production",
    "watch": "webpack --mode production --watch",
    "build-copy": "yarn build && (cat dist/bundle.js | pbcopy) && du -sh dist/*.js",
    "size": "npm run build && size-limit"
  },
  "browserslist": [
    "ie 6"
  ],
  "size-limit": [
    {
      "path": "dist/bundle.js",
      "limit": "100 kb",
      "gzip": false
    }
  ],
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@babel/plugin-transform-arrow-functions": "7.16.0",
    "@babel/preset-env": "7.16.4",
    "@size-limit/preset-big-lib": "7.0.4",
    "@types/firebase": "3.2.1",
    "@types/lodash": "4.14.177",
    "@types/uuid": "8.3.3",
    "babel-loader": "8.2.3",
    "brotli-webpack-plugin": "1.1.0",
    "compression-webpack-plugin": "9.2.0",
    "mobx-keystone": "0.64.0",
    "size-limit": "7.0.4",
    "ts-loader": "9.2.6",
    "typed-emitter": "1.4.0",
    "typescript": "4.5.2",
    "webpack": "5.65.0",
    "webpack-cli": "4.9.1"
  },
  "dependencies": {
    "@fermuch/telematree": "7.2.0",
    "cross-fetch": "3.1.4",
    "firebase": "8.10.0",
    "lodash": "4.17.21",
    "mobx": "6.3.8"
  }
}